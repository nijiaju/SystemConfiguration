execute pathogen#infect()
syntax on
filetype plugin indent on
set number
set background=dark
"set textwidth=80
set shiftwidth=4
set tabstop=4
set laststatus=2
set cindent

let g:airline#extensions#tabline#enabled = 1
let g:tex_flavor='latex'
let g:Tex_TreatMacViewerAsUNIX = 1
let g:Tex_ViewRule_pdf = 'open -a Preview'


":set autowrite
":set hlsearch
":set incsearch
":colorscheme desert
